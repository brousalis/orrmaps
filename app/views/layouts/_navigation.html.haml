%div.navbar.primary.navbar-fixed-top
  %nav.navbar-inner
    .container
      %a.brand.logo{:href => "/"} Orrmaps
      %ul.nav
        %li.your
          %a{:href => "/"}= t("navigation.your_map")
        %li.serv
          - if session[:server]
            %a{:href => "/server/#{underscore(session[:server])}"}= t("navigation.server_map")
          - else
            %a{:href => "/server/jade_quarry"}= t("navigation.server_map")
        %li.rated
          %a.top_rated{:href => "#rated", :"data-toggle" => "modal"}= t("secondary.map_list").humanize
      %ul.nav.pull-right.user
        %li
          %form{:action => "https://www.paypal.com/cgi-bin/webscr", :method => "post"}
            %input{:name => "cmd", :type => "hidden", :value => "_s-xclick"}
              %input{:name => "encrypted", :type => "hidden", :value => "-----BEGIN PKCS7----------END PKCS7-----\r\n"}
                %input{:alt => "PayPal - The safer, easier way to pay online!", :border => "0", :name => "submit", :src => "/assets/donate.png", :type => "image"}
                  %img{:alt => "", :border => "0", :height => "1", :src => "https://www.paypalobjects.com/en_US/i/scr/pixel.gif", :width => "1"} 
        - unless current_user
          %li
            %a.sign_in{:href => "#sign_in", :"data-toggle" => "modal"}
              %i.icon.icon-white.icon-user
              = t("secondary.sign_in")
        - else
          %li
            %a.sign_in.dropdown-toggle{:href => "#", :"data-toggle" => "dropdown"}
              %i.icon.icon-white.icon-user
              %strong= current_user.name
              %b.caret
            %ul.dropdown-menu{:role => "menu", :"aria-labelledby" => "dLabel"}
              %li 
                %a{:href => "#faq", :"data-toggle" => "modal"}
                  %i.icon.icon-white.icon-question-sign
                  = t("secondary.faq") 
              %li
                %a{:href => "/user/delete"}
                  %i.icon.icon-white.icon-remove
                  = t("navigation.delete_account") 
              %li
                %a{:href => "/logout"}
                  %i.icon.icon-white.icon-off
                  = t("navigation.sign_out")  
        %li
          %a{:href => "#servers", :"data-toggle" => "modal"}
            %i.icon.icon-white.icon-home
            %span.server= @server.name
            %b.caret
